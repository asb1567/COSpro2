#include <stdio.h>

int main(void)
{
	float num1;
	double num2;
	long double num3;

	printf("실수 입력1 ( e표기법으로 ) :");
	scanf_s("%f", &num1);
	printf("입력한 실수 %f \n", num1);

	printf("실수 입력2 ( e표기법으로 ) :");
	scanf_s("%lf", &num2);
	printf("입력한 실수 %lf \n", num2);

	printf("실수 입력3 ( e표기법으로 ) :");
	scanf_s("%Lf", &num3);
	printf("입력한 실수 %Lf \n", num3);

	return 0;
}

